@charset "UTF-8";

#qnaProdPic-container-head {
	text-align: center;
}
#qnaProdPic-container-body {
	width: 200px;
	height: 200px;
	border: 1px solid black;
}
#qnaProdPic-container-body img {
  max-width: 100%;
  max-height: 100%;
  object-fit: contain;
  display: block;
  margin: auto;
}

.container-main {
	border: 1px solid black;
	width: 1000px;
	height: 600px;
	
	position: absolute;
	left: 50%;
	transform: translate(-500px);
}

#qnaNotice {
	width: 800px;
	height: 200px;
	border: 3px solid rgb(222, 222, 222);
}